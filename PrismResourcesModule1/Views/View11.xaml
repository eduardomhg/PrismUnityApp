<views:BaseView x:Class="PrismResourcesModule1.Views.View11"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mvvm="http://prismlibrary.com/"
             xmlns:viewModels="clr-namespace:PrismResourcesModule1.ViewModels"
             xmlns:views="clr-namespace:PrismResourcesModule1.Views"
             mvvm:ViewModelLocator.AutoWireViewModel="True"
             d:DataContext="{d:DesignInstance viewModels:View11ViewModel, IsDesignTimeCreatable=True}"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="{Binding Text, Converter={StaticResource SharedConverter}}"/>
            <TextBlock Text="{StaticResource TestResourceKey}"/>
        </StackPanel>
    </Grid>
</views:BaseView>

