<Application x:Class="PrismResourcesApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Styles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/PrismResourcesShared;component/Themes/SharedResources.xaml"/>

                <ResourceDictionary Source="pack://application:,,,/PrismResourcesModule1;component/Themes/SharedResources.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/PrismResourcesModule2;component/Themes/SharedResources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
